
## cf the simple example at https://github.com/spotify/annoy

library(RcppAnnoy)

set.seed(123)                           # be reproducible

f <- 40
annoyCreateIndex(f)

n <- 150                                # not specified

for (i in seq(n)) {
    v <- rnorm(f)
    annoyAddItem(i-1, v)
}

annoyBuild(n)                           # 50 trees
annoySave("/tmp/test.tree")

